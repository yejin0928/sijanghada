<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".thirdActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="431dp"
        android:layout_height="605dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/bha3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333"
        tools:context=".MainActivity" android:fontFamily="@font/nanum">


        <Button
            android:id="@+id/btn_point"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="28dp"
            android:layout_marginRight="28dp"
            android:layout_marginBottom="8dp"
            android:background="#F4E478"
            android:onClick="onClick"
            android:text="포인트 적립"
            app:layout_constraintBottom_toTopOf="@+id/btn_feed"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:fontFamily="@font/nanum"/>

        <Button
            android:id="@+id/btn_feed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="28dp"
            android:layout_marginRight="28dp"
            android:layout_marginBottom="150dp"
            android:background="#F4E478"
            android:onClick="onClick"
            android:text="리뷰 남기기"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:fontFamily="@font/nanum"/>

        <Button
            android:id="@+id/m1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:background="#BFC638"
            android:onClick="onClick"
            android:text="효자지하생선"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="@font/nanum"/>

        <Button
            android:id="@+id/m6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="35dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#BFC638"
            android:onClick="onClick"
            android:text="현대한우전문점"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="@font/nanum"/>

        <Button
            android:id="@+id/m2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="170dp"
            android:background="#BFC638"
            android:onClick="onClick"
            android:text="효자청과마트"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="@font/nanum"/>

        <Button
            android:id="@+id/m7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="150dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#BFC638"
            android:onClick="onClick"
            android:text="개성상회"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="@font/nanum"/>

        <Button
            android:id="@+id/m3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="310dp"
            android:background="#BFC638"
            android:onClick="onClick"
            android:text="통인옷집"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="@font/nanum"/>

        <Button
            android:id="@+id/m8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="330dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#BFC638"
            android:onClick="onClick"
            android:text="호남상회"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="@font/nanum"/>

        <Button
            android:id="@+id/m4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="430dp"
            android:background="#BFC638"
            android:onClick="onClick"
            android:text="송림정육점"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="@font/nanum"/>

        <Button
            android:id="@+id/m9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="430dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#BFC638"
            android:onClick="onClick"
            android:text="고기백화점"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="@font/nanum"/>

        <Button
            android:id="@+id/m5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="535dp"
            android:background="#BFC638"
            android:onClick="onClick"
            android:text="푸른마트"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="@font/nanum"/>

        <Button
            android:id="@+id/m10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="535dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#BFC638"
            android:onClick="onClick"
            android:text="BBQ올리브떡볶이"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:fontFamily="@font/nanum"/>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>